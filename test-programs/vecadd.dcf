class Program{
	int a[100],b[100],c[100];
	void main()
	{
		int n,i;
		callout("printf","Enter size of array\n");
		n=5;
		callout("printf","Enter elements of first array\n");
		for i=0,n-1{
			a[i]=i*2;
		}
		callout("printf","Enter elements of second array\n");
		for i=0,n-1{
			b[i]=i*3;
		}
		for i=0,n-1{
			c[i] = a[i]+b[i];
			callout("printf","%d ",c[i]);
		}
		callout("printf","\n");
	}
}
